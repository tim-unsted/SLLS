@model slls.ViewModels.TitleCustomDataIndexViewModel

@Html.ValidationSummary(true, "", new { @class = "text-danger" })
@Html.HiddenFor(model => model.FieldId)
@Html.HiddenFor(model => model.TitleId)
@Html.HiddenFor(model => model.RecId)

<div class="form-group">
    <div class="col-md-12">
        @Html.Label(Model.FieldName)
        @(Html.EditorFor(model => model.FieldData, new { htmlAttributes = new { @class = "form-control" } }))
        @Html.ValidationMessageFor(model => model.FieldData, "", new { @class = "text-danger" })
    </div>
</div>