@model slls.ViewModels.DashboardViewModel

<div class="panel panel-gadget shadowed">
    <div class="panel-heading">
        <span class="panel-title">
            <span is="gadget-Title">@ViewBag.Title</span>
        </span>
    </div>
    <div class="panel-body">
        <table class="table table-compact">
            <thead>
            <tr>
                <th>
                    Item
                </th>
            </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.IssuesExpected)
                {
                    <tr>
                        <td>
                            <a href="@Url.Action("QuickCheckIn", "PartsReceived", new {selectedCopy = item.CopyID})" class="btn-link" title="View in Quick Check-In">@item.Title.Title1</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
