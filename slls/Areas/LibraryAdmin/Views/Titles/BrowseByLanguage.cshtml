@model slls.ViewModels.TitlesListViewModel

@Html.Partial("_HeaderSeeAlso", Model)

@using (Html.BeginForm(null, null, FormMethod.Get, new { id = "TitlesTable" }))
{
    <div class="well well-default">
        <div class="form-group">
            @Html.DropDownList("ListLanguage", null, new { @class = "form-control", onchange = "this.form.submit();" })
        </div>
    </div>
}
<div id="TitlesTable">
    @Html.Partial("_Index", Model)
</div>

@section scripts {
@*@Scripts.Render("~/bundles/datatables")*@
    <script src="@Url.Content("~/DataTables/LibraryAdmin/Titles/_Index.js")" type="text/javascript"></script>

}



