    @using (Html.BeginForm(null, null, FormMethod.Get, new { id = "partslist" }))
    {
        <div class="form-group">
            @Html.DropDownList("SelectedCopy", null, new { @class = "form-control", onchange = "this.form.submit();" })
        </div>
